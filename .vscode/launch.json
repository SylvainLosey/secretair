{
    "version": "0.2.0",
    "configurations": [ 
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Next.js",
        "skipFiles": ["<node_internals>/**"],
        "runtimeArgs": ["dev"],
        "program": "${workspaceFolder}/node_modules/.bin/next",
        "args": ["dev"],
        "cwd": "${workspaceFolder}",
        "env": {
          "NODE_ENV": "development"
        }
      }
    ]
  }